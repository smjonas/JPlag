<?xml version="1.0" encoding="ASCII"?>
<sgraph:Statechart xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:sgraph="http://www.yakindu.org/sct/sgraph/2.0.0" specification="internal:&#xA;  var t: integer&#xA;&#xA;interface user:&#xA;  in event press_button" name="Statechart">
  <regions name="main region">
    <vertices xsi:type="sgraph:State" specification="entry / t = 0" name="Blinking" incomingTransitions="//@regions.0/@vertices.1/@outgoingTransitions.0">
      <outgoingTransitions specification="user.press_button&#xA;" target="//@regions.0/@vertices.1"/>
      <regions name="blinking">
        <vertices xsi:type="sgraph:State" specification="" name="Dark">
          <outgoingTransitions specification="after 1s "/>
          <outgoingTransitions specification="[t == 5]" target="//@regions.0/@vertices.1"/>
        </vertices>
      </regions>
      <regions name="blinking">
        <vertices xsi:type="sgraph:State" specification="entry / t++" name="Light" incomingTransitions="//@regions.0/@vertices.0/@regions.0/@vertices.2/@outgoingTransitions.0 //@regions.0/@vertices.0/@regions.0/@vertices.1/@outgoingTransitions.1">
          <outgoingTransitions specification="after 1s " target="//@regions.0/@vertices.0/@regions.0/@vertices.1"/>
        </vertices>
        <vertices xsi:type="sgraph:State" specification="" name="Dark" incomingTransitions="//@regions.0/@vertices.0/@regions.0/@vertices.0/@outgoingTransitions.0">
          <outgoingTransitions specification="[t == 5]"/>
          <outgoingTransitions specification="after 1s " target="//@regions.0/@vertices.0/@regions.0/@vertices.0"/>
        </vertices>
        <vertices xsi:type="sgraph:Entry" kind="SHALLOW_HISTORY">
          <outgoingTransitions specification="" target="//@regions.0/@vertices.0/@regions.0/@vertices.0"/>
        </vertices>
      </regions>
    </vertices>
    <vertices xsi:type="sgraph:State" specification="" name="Start" incomingTransitions="//@regions.0/@vertices.2/@outgoingTransitions.0 //@regions.0/@vertices.0/@outgoingTransitions.0 //@regions.0/@vertices.0/@regions.1/@vertices.0/@outgoingTransitions.1">
      <outgoingTransitions specification="user.press_button&#xA;" target="//@regions.0/@vertices.0"/>
    </vertices>
    <vertices xsi:type="sgraph:Entry">
      <outgoingTransitions target="//@regions.0/@vertices.1"/>
    </vertices>
  </regions>
</sgraph:Statechart>
