<?xml version="1.0" encoding="UTF-8"?>
<scxml xmlns="http://www.w3.org/2005/07/scxml" version="1.0" datamodel="ecmascript" name="Statechart">
	<datamodel>
		<data expr="0" id="t" />
	</datamodel>
	<state id="main_region">
		<initial>
			<transition target="Start" type="internal">
			</transition>
		</initial>
		<state id="Start">
			<transition event="user.press_button"  target="Blinking">
			</transition>
		</state>
		<state id="Blinking">
			<onentry>
				<assign location="t" expr="0"/>
			</onentry>
			<initial>
				<transition target="Blinking.entry_0" type="internal">
				</transition>
			</initial>
			<state id="Light">
				<onentry>
					<assign location="t" expr="t + 1"/>
				</onentry>
				<transition   target="Dark">
				</transition>
			</state>
			<state id="Dark">
				<onentry>
					<send event="Dark_t_1_timeEvent_0" delay="1s"/>
				</onentry>
				<onexit>
					<cancel sendid="Dark_t_1_timeEvent_0" />
				</onexit>
				<transition  cond="t == 5" target="Start">
				</transition>
				<transition event="Dark_t_1_timeEvent_0"  target="Light">
				</transition>
			</state>
			<history type = "shallow" id="Blinking.entry_0">
				<transition   target="Light">
				</transition>
			</history>
			<transition event="user.press_button"  target="Start">
			</transition>
		</state>
	</state>
</scxml>